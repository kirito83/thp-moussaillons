<h1>Nouveau Corsair</h1>

<br/>

<%= form_for @corsair do |f| %>
	<%= render 'layouts/form' %>
  	<div class='form-group'>
		<label>Prénom</label>
		<%= f.text_field :first_name, class: 'form-control' %>
	</div>
	<div class='form-group'>
		<label>Âge</label>
		<%= f.text_field :age, class: 'form-control' %>
	</div>
	<br/>
	<div class='form-group'>
		<button type='submit' class='btn btn-primary'>Création Corsair</button>
	</div>
<% end %>